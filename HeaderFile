#include <iostream>
#include <cmath>
#include <string>
#include <algorithm>
#include <vector>
#include <limits>
#include <unordered_map>

using namespace std;

// Function to display the graph (for debugging and visualization)
void displayGraph(int graph[30][30], int num_v) {
    cout << "Graph adjacency matrix:\n";
    for (int i = 0; i < num_v; i++) {
        for (int j = 0; j < num_v; j++) {
            cout << graph[i][j] << " ";
        }
        cout << endl;
    }
}
